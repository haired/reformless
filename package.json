{
  "name": "reformless",
  "version": "0.1.2",
  "description": "Reformless helps you build form in React fast, easily, painlessly and without all the usual boilerplate.",
  "author": "Christian Kouame <christian.kouame@live.fr>",
  "license": "MIT",
  "repository": "haired/reformless",
  "keywords": [
    "reformless",
    "react",
    "react-dom",
    "form",
    "validation",
    "forms"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.js",
  "dependencies": {},
  "module": "src/index.js",
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "lint": "eslint src/**.{ts,tsx,js,jsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "peerDependencies": {
    "react": ">= 15.0.0",
    "react-dom": ">= 15.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/types": "^7.9.5",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "roots": [
      "<rootDir>tests"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>setupTest.ts"
    ]
  }
}